 
 
 
 ALGORITHM insertion_sort
 VAR
     i ,j,key:= INTEGER;


 BEGIN
    FOR i FROM 0 TO    DO
        key:=arr[i];
        j:=i-1 ;

        IF (j>=0 && arr[j]>key) THEN
            arr[j+1] :=aar[j];
            j:=j+1
        END_IF
        arr[j+1]:=key
    END_FOR
 END
 
      


